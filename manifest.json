{
  "manifest_version": 2,
  "name": "translateil",
  "version": "1.0.0",
  "description": "quick translate text",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  }
}
